
<!DOCTYPE html>
<html lang="en">
<head>
    <title>AdminX - The last Admin template you'll ever need</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="stylesheet" type="text/css" href="~/admin/dist/css/adminx.css" media="screen" />

    <!-- If you prefer jQuery these are the required scripts -->
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js"></script>
    
    <!--
      # Optional Resources
      Feel free to delete these if you don't need them in your project
    -->
</head>
<body>
    <div class="adminx-container">
        <nav class="navbar navbar-expand justify-content-between fixed-top">
            <a class="navbar-brand mb-0 h1 d-none d-md-block" href="index.html">
                <img src="~/admin/demo/img/logo.png" class="navbar-brand-image d-inline-block align-top mr-2" alt="">
                Rent a Car
            </a>

            <form class="form-inline form-quicksearch d-none d-md-block mx-auto">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <div class="input-group-icon">
                            <i data-feather="search"></i>
                        </div>
                    </div>
                    <input type="text" class="form-control" id="search" placeholder="Type to search...">
                </div>
            </form>

            <div class="d-flex flex-1 d-block d-md-none">
                <a href="#" class="sidebar-toggle ml-3">
                    <i data-feather="menu"></i>
                </a>
            </div>

            <ul class="navbar-nav d-flex justify-content-end mr-2">
                <!-- Notificatoins -->
                <li class="nav-item dropdown d-flex align-items-center mr-2">
                    <a class="nav-link nav-link-notifications" id="dropdownNotifications" data-toggle="dropdown" href="#">
                        <i class="oi oi-bell display-inline-block align-middle"></i>
                        <span class="nav-link-notification-number">3</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right dropdown-menu-notifications" aria-labelledby="dropdownNotifications">
                        <div class="notifications-header d-flex justify-content-between align-items-center">
                            <span class="notifications-header-title">
                                Notifications
                            </span>
                            <a href="#" class="d-flex"><small>Mark all as read</small></a>
                        </div>

                        <div class="list-group">
                            <a href="#" class="list-group-item list-group-item-action unread">
                                <p class="mb-1">Invitation for <strong>Lunch</strong> on <strong>Jan 12th 2018</strong> by <strong>Laura</strong></p>

                                <div class="mb-1">
                                    <button type="button" class="btn btn-primary btn-sm">Accept invite</button>
                                    <button type="button" class="btn btn-outline-danger btn-sm">Decline</button>
                                </div>

                                <small>1 hour ago</small>
                            </a>

                            <a href="#" class="list-group-item list-group-item-action">
                                <p class="mb-1"><strong class="text-success">Goal completed</strong><br />1,000 new members today</p>
                                <small>3 days ago</small>
                            </a>

                            <a href="#" class="list-group-item list-group-item-action">
                                <p class="mb-1 text-danger"><strong>System error detected</strong></p>
                                <small>3 days ago</small>
                            </a>

                            <a href="#" class="list-group-item list-group-item-action">
                                <p class="mb-1">Your task <strong>Design Draft</strong> is due today.</p>
                                <small>4 days ago</small>
                            </a>
                        </div>

                        <div class="notifications-footer text-center">
                            <a href="#"><small>View all notifications</small></a>
                        </div>
                    </div>
                </li>
                <!-- Notifications -->
                <li class="nav-item dropdown">
                    <a class="nav-link avatar-with-name" id="navbarDropdownMenuLink" data-toggle="dropdown" href="#">
                        <img src="https://s3.amazonaws.com/uifaces/faces/twitter/jsa/128.jpg" class="d-inline-block align-top" alt="">
                    </a>
                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                        <a class="dropdown-item" href="#">My Profile</a>
                        <a class="dropdown-item" href="#">My Tasks</a>
                        <a class="dropdown-item" href="#">Settings</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item text-danger" asp-controller="Auth" asp-action="Logout">Sign out</a>
                    </div>
                </li>
            </ul>
        </nav>

        <!-- expand-hover push -->
        <!-- Sidebar -->
        <div class="adminx-sidebar expand-hover push">
            <ul class="sidebar-nav">
                <li class="sidebar-nav-item">
                    <a asp-action="Index" asp-controller="Home" class="sidebar-nav-link active">
                        <span class="sidebar-nav-icon">
                            <i data-feather="home"></i>
                        </span>
                        <span class="sidebar-nav-name">
                            Yönetim Paneli
                        </span>
                        <span class="sidebar-nav-end">

                        </span>
                    </a>
                </li>


                <li class="sidebar-nav-item">
                    <a class="sidebar-nav-link collapsed" data-toggle="collapse" href="#example" aria-expanded="false" aria-controls="example">
                        <span class="sidebar-nav-icon">
                            <i data-feather="pie-chart"></i>
                        </span>
                        <span class="sidebar-nav-name">
                            Araç Yönetimi
                        </span>
                        <span class="sidebar-nav-end">
                            <i data-feather="chevron-right" class="nav-collapse-icon"></i>
                        </span>
                    </a>

                    <ul class="sidebar-sub-nav collapse" id="example">
                        <li class="sidebar-nav-item">
                            <a asp-action="Index" asp-controller="Car" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    AL
                                </span>
                                <span class="sidebar-nav-name">
                                    Araç Listesi
                                </span>
                            </a>
                        </li>

                        <li class="sidebar-nav-item">
                            <a asp-action="Add" asp-controller="Car" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    AE
                                </span>
                                <span class="sidebar-nav-name">
                                    Araç Ekle
                                </span>
                            </a>
                        </li>
                        <li class="sidebar-nav-item">
                            <a asp-controller="Car" asp-action="DeletedCar" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    PA
                                </span>
                                <span class="sidebar-nav-name">
                                    Pasif Araçlar
                                </span>
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="sidebar-nav-item">
                    <a class="sidebar-nav-link collapsed" data-toggle="collapse" href="#navTables" aria-expanded="false" aria-controls="navTables">
                        <span class="sidebar-nav-icon">
                            <i data-feather="align-justify"></i>
                        </span>
                        <span class="sidebar-nav-name">
                            Kategoriler
                        </span>
                        <span class="sidebar-nav-end">
                            <i data-feather="chevron-right" class="nav-collapse-icon"></i>
                        </span>
                    </a>

                    <ul class="sidebar-sub-nav collapse" id="navTables">
                        <li class="sidebar-nav-item">
                            <a asp-controller="Category" asp-action="Index" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    KL
                                </span>
                                <span class="sidebar-nav-name">
                                    Kategori Listesi
                                </span>
                            </a>
                        </li>

                        <li class="sidebar-nav-item">
                            <a asp-action="AddCategory" asp-controller="Category" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    KE
                                </span>
                                <span class="sidebar-nav-name">
                                    Kategori Ekle
                                </span>
                            </a>
                        </li>
                        <li class="sidebar-nav-item">
                            <a asp-action="DeletedCategories" asp-controller="Category" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    PK
                                </span>
                                <span class="sidebar-nav-name">
                                    Pasif Kategoriler
                                </span>
                            </a>
                        </li>
                    </ul>
                </li>
                <li class="sidebar-nav-item">
                    <a class="sidebar-nav-link collapsed" data-toggle="collapse" href="#navExtra" aria-expanded="false" aria-controls="navExtra">
                        <span class="sidebar-nav-icon">
                            <i data-feather="layers"></i>
                        </span>
                        <span class="sidebar-nav-name">
                            Marka Yönetimi
                        </span>
                        <span class="sidebar-nav-end">
                            <i data-feather="chevron-right" class="nav-collapse-icon"></i>
                        </span>
                    </a>

                    <ul class="sidebar-sub-nav collapse" id="navExtra">
                        <li class="sidebar-nav-item">
                            <a asp-action="Index" asp-controller="Brand" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    ML
                                </span>
                                <span class="sidebar-nav-name">
                                    Marka Listesi
                                </span>
                            </a>
                        </li>

                        <li class="sidebar-nav-item">
                            <a asp-action="Add" asp-controller="Brand" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    ME
                                </span>
                                <span class="sidebar-nav-name">
                                    Marka Ekle
                                </span>
                            </a>
                        </li>

                        <li class="sidebar-nav-item">
                            <a asp-action="DeletedBrands" asp-controller="Brand" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    PM
                                </span>
                                <span class="sidebar-nav-name">
                                    Pasif Markalar
                                </span>
                            </a>
                        </li>                       
                    </ul>
                </li>

                
                

                <li class="sidebar-nav-item">
                    <a class="sidebar-nav-link collapsed" data-toggle="collapse" href="#navForms" aria-expanded="false" aria-controls="navForms">
                        <span class="sidebar-nav-icon">
                            <i data-feather="users"></i>
                        </span>
                        <span class="sidebar-nav-name">
                            Kullanıcı Yönetimi
                        </span>
                        <span class="sidebar-nav-end">
                            <i data-feather="chevron-right" class="nav-collapse-icon"></i>
                        </span>
                    </a>

                    <ul class="sidebar-sub-nav collapse" id="navForms">
                        <li class="sidebar-nav-item">
                            <a asp-action="Index" asp-controller="User" asp-area="Admin" class="sidebar-nav-link">
                                <span class="sidebar-nav-abbr">
                                    KY
                                </span>
                                <span class="sidebar-nav-name">
                                    Kullanıcılar
                                </span>
                            </a>
                        </li>
                    </ul>
                </li>

                <li class="sidebar-nav-item">
                    <a class="sidebar-nav-link collapsed" asp-action="Index" asp-controller="Rental" asp-area="Admin">
                        <span class="sidebar-nav-icon">
                            <i data-feather="grid"></i>
                        </span>
                        <span class="sidebar-nav-name">
                            Kiralama Hareketleri
                        </span>
                        <span class="sidebar-nav-end">
                            <i data-feather="chevron-right" class="nav-collapse-icon"></i>
                        </span>
                    </a>

                    
                </li>
                <li class="sidebar-nav-item">
                    <a class="sidebar-nav-link collapsed" asp-action="RefundCancellationRequests" asp-controller="Rental" asp-area="Admin">
                        <span class="sidebar-nav-icon">
                            <i data-feather="grid"></i>
                        </span>
                        <span class="sidebar-nav-name">
                            Ödeme Iade Talepleri
                        </span>
                        <span class="sidebar-nav-end">
                            <i data-feather="chevron-right" class="nav-collapse-icon"></i>
                        </span>
                    </a>


                </li>

                
            </ul>
        </div><!-- Sidebar End -->
        <!-- adminx-content-aside -->
        <div class="adminx-content">
            <!-- <div class="adminx-aside">

            </div> -->

            @RenderBody()
        </div>
    </div>
    <script src="~/admin/dist/js/vendor.js"></script>
    <script src="~/admin/dist/js/adminx.js"></script>

    <!-- If you prefer vanilla JS these are the only required scripts -->
    <!-- script src="~/admin/dist/js/vendor.js"></script>
    <script src="~/admin/dist/js/adminx.vanilla.js"></script-->
        @RenderSection("Scripts",required: false);
          @await Component.InvokeAsync("NToastNotify")
</body>
</html>